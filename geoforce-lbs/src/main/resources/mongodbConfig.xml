<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd" >

<beans>
 	<bean id="mongoDbGlobal" class="com.supermap.egispservice.lbs.mongobase.MongoDbGlobal">
  		<property name="mongoUrl">
  			<!-- 外测地址 -->
  			<!-- <value>121.40.86.232</value> -->
			<!-- 本地测地址 -->
  			 <value>${mongo.url}</value>
  		</property>
  		<property name="dbName">
  			<value>${mongo.db}</value>
  		</property>
  		<property name="dbUser">
  			<value>${mongo.username}</value>
  		</property>
  		<property name="dbPwd">
  			<value>${mongo.password}</value>
  		</property>
 	</bean>
</beans>
